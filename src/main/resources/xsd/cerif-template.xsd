			<xs:complexContent>
				<xs:extension base="ProjectFunding__BaseType">
					<xs:sequence minOccurs="0">
						
						<xs:element name="Type" type="cfGenericURIClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="https://w3id.org/cerif/model#Funding_Classification" cfprocess:expandBy="Classification(Type)">
