						<xs:element name="Subject" type="cfGenericURIClassification__Type" minOccurs="0" maxOccurs="unbounded" cflink:link="https://w3id.org/cerif/model#ResultProduct_Classification" cfprocess:expandBy="Classification(Subject)">
