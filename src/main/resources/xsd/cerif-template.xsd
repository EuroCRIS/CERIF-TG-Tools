								<xs:documentation>A description of the funding</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="Keyword" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="https://w3id.org/cerif/model#Funding.Keywords">
							<xs:annotation>
								<xs:documentation>A single keyword or key expression. Please repeat to serialize separate keywords or key expressions.</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="Funder" minOccurs="0" maxOccurs="unbounded" cflink:link="https://w3id.org/cerif/model#OrganisationUnit_Funding(https://w3id.org/cerif/vocab/OrganisationFundingEngagements#Funder)" type="cfLinkWithDisplayNameToPersonOrOrgUnit__Type">
							<xs:annotation>
								<xs:documentation>The funder or funders</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="PartOf" minOccurs="0" cflink:link="https://w3id.org/cerif/model#Funding_Funding(https://w3id.org/cerif/vocab/Inter-­FundingRelations#Part):1">
							<xs:annotation>
								<xs:documentation>Chain up to the larger funding that encompasses this funding</xs:documentation>
							</xs:annotation>
							<xs:complexType>
