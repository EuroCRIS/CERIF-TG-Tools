						<xs:element name="VersionInfo" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="https://w3id.org/cerif/model#ResultProduct.VersionInfo"/>
						
						<xs:element name="Identifier" type="cfGenericIdentifier__Type" minOccurs="0" maxOccurs="unbounded" cflink:identifier="true" cfprocess:expandBy="https://w3id.org/cerif/vocab/IdentifierTypes#">
							<xs:annotation>
								<xs:documentation>An identifier of this product (NOT of the research output this product is part of)</xs:documentation>
							</xs:annotation>
						</xs:element>

						<xs:element cflink:container="ordered" name="Creators" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The creators of this product</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Creator" type="cfLinkWithDisplayNameToPersonWithAffiliationsOrOrgUnit__Type" minOccurs="0" maxOccurs="unbounded" 
