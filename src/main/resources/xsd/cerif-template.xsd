						<xs:element name="Acronym" type="cfString__Type" minOccurs="0" cflink:attribute="https://w3id.org/cerif/model#Funding.Acronym">
							<xs:annotation>
								<xs:documentation>The acronym of the funding</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="Name" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="https://w3id.org/cerif/model#Funding.Name">
							<xs:annotation>
								<xs:documentation>The name of the funding</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="Amount" type="cfAmount__Type" minOccurs="0" cflink:attribute="https://w3id.org/cerif/model#Funding.Amount https://w3id.org/cerif/model#Funding.CurrencyCode">
							<xs:annotation>
								<xs:documentation>The amount of the funding and its currency</xs:documentation>
							</xs:annotation>
						</xs:element>
						
						<xs:element name="Description" type="cfMLangString__Type" minOccurs="0" maxOccurs="unbounded" cflink:attribute="https://w3id.org/cerif/model#Funding.Description">
